//Jackie Cai countdown program
//z5259449 1/10/2019

#include<stdio.h>
#include<stdlib.h>

int main(void){
    int num;
    int counter = 1;
    printf("Enter number: ");
    scanf("%d", &num);
    while (num > counter){
    if (counter%3 == 0 || counter%5 == 0){
    printf("%d\n",counter);
    }
     counter = counter + 1;    
    }
    return 0;
}
