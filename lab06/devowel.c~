#include<stdio.h>
#include<ctype.h>
int main(void){
    int ch = getchar();
    while(ch != EOF){
        if(ch == (ch == 'a' && ch == 'e' 
        && ch == 'i' && ch == 'o' && ch == 'u')){ 
            printf("%c" ,ch);
        }
        ch = getchar();
    }
}
