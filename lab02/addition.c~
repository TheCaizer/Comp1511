#include<stdio.h>
#include<stdlib.h>
 int main(void) {
    int nOne;
    int nTwo;
    int sum;

    printf("Please enter two integers: \n");
    scanf ("%d",&nOne);
    scanf ("%d",&nTwo);
    sum = nOne + nTwo;
    printf("%d + %d = %d \n",nOne ,nTwo ,sum);
    return 0;
 }
